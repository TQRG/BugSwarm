<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:ti="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" name="SA00077-CompensateTargetScopeNestedInCompensationHandler" targetNamespace="http://dsg.wiai.uniba.de/betsy/rules/sa00077/bpel/SA00077-CompensateTargetScopeNestedInCompensationHandler">
    <import namespace="http://dsg.wiai.uniba.de/betsy/activities/wsdl/testinterface" location="TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="InitData" messageType="ti:executeProcessSyncRequest"/>
    </variables>
    <faultHandlers>
        <catchAll>
            <compensateScope name="CompensateScope" target="NestedScope"/>
        </catchAll>
    </faultHandlers>
    <sequence name="Sequence">
        <scope name="Scope">
            <compensationHandler>
                <sequence>
                    <scope name="NestedScope">
                        <compensationHandler>
                            <compensate/>
                        </compensationHandler>
                        <empty/>
                    </scope>
                    <reply name="ReplyToInitialReceive" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="ReplyData"/>
                </sequence>
            </compensationHandler>
            <sequence>
                <receive name="InitialReceive" createInstance="yes" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="InitData"/>
                <assign>
                    <copy ignoreMissingFromData="yes">
                        <from variable="InitData" part="inputPart"/>
                        <to variable="ReplyData" part="outputPart"/>
                    </copy>
                </assign>
            </sequence>
        </scope>
        <throw name="Throw" faultName="bpel:completionConditionFailure"/>
    </sequence>
</process>
