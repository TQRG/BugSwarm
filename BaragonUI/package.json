{
  "name": "BaragonUI",
  "description": "UI For the load balancer api Baragon",
  "version": "0.1.0",
  "homepage": "https://github.com/HubSpot/Baragon",
  "repository": {
    "type": "git",
    "url": "git@github.com:HubSpot/Baragon.git"
  },
  "engines": {
    "node": "~4.1.2",
    "npm": "~2.14.16"
  },
  "bin": {
    "bower": "./node_modules/bower/bin/bower",
    "brunch": "./node_modules/brunch/bin/brunch"
  },
  "scripts": {
    "start": "brunch watch --server",
    "postinstall": "bower install --allow-root && brunch build --production"
  },
  "dependencies": {
    "bower": "~1.7.9",
    "brunch": "~2.2.3",
    "clean-css-brunch": ">= 1.0 < 1.8",
    "coffee-script-brunch": "~2.0.0",
    "css-brunch": "~2.6.1",
    "handlebars-brunch": "~2.0.1",
    "javascript-brunch": "~2.0.0",
    "less-brunch": "~2.6.0",
    "stylus-brunch": "~2.6.0",
    "uglify-js-brunch": "~2.0.1"
  }
}
