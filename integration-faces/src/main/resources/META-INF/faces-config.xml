<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<name>rewrite_integration_faces</name>

	<application>
		<navigation-handler>org.ocpsoft.rewrite.faces.RewriteNavigationHandler</navigation-handler>
		<view-handler>org.ocpsoft.rewrite.faces.RewriteViewHandler</view-handler>
	</application>

	<lifecycle>
		<phase-listener>org.ocpsoft.rewrite.faces.RewritePhaseListener</phase-listener>
	</lifecycle>

</faces-config>
